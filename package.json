{
  "name": "i3s-server",
  "version": "1.0.4",
  "description": "A lightweight I3S (Indexed 3D Scene Service) server built with Elysia, designed to serve 3D scene data from SLPK (Scene Layer Package) archives",
  "author": {
    "email": "admin@rikka.cc",
    "name": "Rikka",
    "url": "https://github.com/Muromi-Rikka"
  },
  "license": "MIT",
  "homepage": "https://github.com/Muromi-Rikka/i3s-server",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Muromi-Rikka/i3s-server.git"
  },
  "bugs": {
    "url": "https://github.com/Muromi-Rikka/i3s-server/issues"
  },
  "keywords": ["i3s", "slpk", "3d-scene-server", "elysia", "bun"],
  "module": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch index.ts",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server index.ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@loaders.gl/i3s": "^4.3.4",
    "@loaders.gl/loader-utils": "^4.3.4",
    "@loaders.gl/polyfills": "^4.3.4",
    "destr": "^2.0.5",
    "elysia": "latest",
    "pathe": "^2.0.3",
    "ufo": "^1.6.1",
    "uuid": "^11.1.0",
    "yaml": "^2.3.4",
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.1",
    "@types/uuid": "^10.0.0",
    "bun-types": "latest",
    "eslint": "^9.29.0"
  }
}
